{
    "name": "minhyung/laravel-synology",
    "description": "Synology API",
    "keywords": [
        "minhyung",
        "laravel-synology"
    ],
    "homepage": "https://github.com/overworks/laravel-synology",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Minhyung Park",
            "email": "urlinee@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Minhyung\\LaravelSynology\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minhyung\\LaravelSynology\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minhyung\\LaravelSynology\\LaravelSynologyServiceProvider"
            ],
            "aliases": {
                "LaravelSynology": "Minhyung\\LaravelSynology\\LaravelSynologyFacade"
            }
        }
    }
}
